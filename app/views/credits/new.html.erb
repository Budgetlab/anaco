<% content_for :title do %>Formulaire création formulaire crédits non répartis | ANACO <% end %>
<main role="main" class="containtAll">
  <div class="fr-container" >
    <nav role="navigation" class="fr-breadcrumb" aria-label="vous êtes ici :">
      <button class="fr-breadcrumb__button " aria-expanded="false" aria-controls="breadcrumb-1" title="voir le fil d'ariane">Voir le fil d’Ariane</button>
      <div class="fr-collapse" id="breadcrumb-1">
        <ol class="fr-breadcrumb__list">
          <li>
            <%= link_to root_path, class:"fr-breadcrumb__link" do %> Accueil <%end %>
          </li>
          <li>
            <%= link_to programmes_path, class:"fr-breadcrumb__link" do %> Programmes et crédits non répartis <%end %>
          </li>
          <li>
            <a class="fr-breadcrumb__link" aria-current="page">Formulaire <%= @form %></a>
          </li>
        </ol>
      </div>
    </nav>
    <div class="fr-grid-row fr-grid-row--gutters">
      <div class="fr-col-12 fr-col-lg-12">
        <h1>Formulaire crédits non répartis <%= @form %> <%= @annee %></h1>
        <div class="fr-h3 fr-mb-0">Programme n°<%= @programme.numero%> </div>
      </div>
    </div>
      <%= render 'form'%>
  </div>
</main>