                <td><%= @hash_phase_lecture["début de gestion"].transpose.map{|e| e.inject(:+)}[2] %></td>
                <td><%= @hash_phase_lecture["début de gestion"].transpose.map{|e| e.inject(:+)}[3] %></td>

              </tr>
              <%  @hash_phase_lecture["début de gestion"].each do |user_array|%>
