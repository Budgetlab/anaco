<div class="fr-grid-row fr-grid-row--gutters fr-grid-row--middle" >
  <div class="fr-col-12 fr-col-lg-1 fr-capitalize">
   NREP <%= phase %>
  </div>

  <div class="fr-col-12 fr-col-lg-9">
    <div class="fr-grid-row fr-grid-row--gutters">

        <div class="fr-col-3">
          <div class="fr-card fr-card--creme fr-p-2w fr-card--no-border">
            <div class="fr-cart__body">
              <p class="fr-text--lg fr-mb-0 text_center">AE HT2</p>
              <p class="fr-text--lg fr-text--bold fr-mb-0 text_center "><%= format_number(credit[phase][0]) %>€</p>
            </div>
          </div>
        </div>
      <div class="fr-col-3">
        <div class="fr-card fr-card--creme fr-p-2w fr-card--no-border">
          <div class="fr-cart__body">
            <p class="fr-text--lg fr-mb-0 text_center">CP HT2</p>
            <p class="fr-text--lg fr-text--bold fr-mb-0 text_center "><%= format_number(credit[phase][1]) %>€</p>
          </div>
        </div>
      </div>
      <div class="fr-col-3">
        <div class="fr-card fr-card--creme fr-p-2w fr-card--no-border">
          <div class="fr-cart__body">
            <p class="fr-text--lg fr-mb-0 text_center">AE/CP T2</p>
            <p class="fr-text--lg fr-text--bold fr-mb-0 text_center "><%= format_number(credit[phase][2]) %>€</p>
          </div>
        </div>
      </div>

    </div>
  </div>
</div>